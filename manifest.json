{
  "manifest_version": 3,
  "name": "ByrutGame URL Fixer",
  "version": "1.0",
  "description": "修复byrutgame.org URL中的多余点号",
  "author": "森哥科技",
  "permissions": [
    "webNavigation",
    "tabs"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.svg",
      "48": "images/icon48.svg",
      "128": "images/icon128.svg"
    }
  },
  "icons": {
    "16": "images/icon16.svg",
    "48": "images/icon48.svg",
    "128": "images/icon128.svg"
  },
  "host_permissions": [
    "*://byrutgame.org/*",
    "*://byrutgame.org./*"
  ]
}